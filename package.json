{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker compose up -d && next dev && docker compose down",
    "launch": "npm run build && npm run database && npm run start",
    "launch:test": "TESTING=test npm run build && npm run database:test && npm run start",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "npx prettier . --write",
    "database:dev": "npm run database && prisma migrate dev && npm run database:down",
    "database:deploy": "prisma migrate deploy",
    "database:check": "prisma migrate diff --to-schema-datamodel prisma/schema.prisma --from-schema-datasource prisma/schema.prisma --exit-code",
    "database": "docker compose up -d",
    "database:down": "docker compose down",
    "database:create": "prisma generate && prisma db push",
    "database:test": "docker compose -f compose-test.yml up -d && npm run database:create",
    "test:unit": "vitest run --dir __tests__/unit --config vitest.config.ts",
    "test:integration": "docker compose -f compose-test.yml up -d && npm run database:create && vitest run --dir __tests__/integration && docker compose -f compose-test.yml down",
    "test:llm": "vitest run --dir __tests__/llm --disable-console-intercept",
    "test:e2e": "npm run database:down && docker compose -f compose-test.yml up -d && npm run database:create && TESTING=test npm run build && start-server-and-test start http://localhost:3000 'cypress run' && docker compose -f compose-test.yml down",
    "test:specific": "cypress run --spec $npm_config_testfile",
    "test": "npm run test:unit && npm run test:integration && npm run test:llm && npm run test:e2e",
    "email": "email dev --port 3002"
  },
